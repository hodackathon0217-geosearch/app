{
  "name": "react-template",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "npm build && node server/index.js",
    "dev": "concurrently --kill-others \"npm run dev-client\" \"npm run dev-server\"",
    "dev-client": "webpack-dev-server",
    "dev-server": "nodemon server/index.js",
    "test": "npm run test-server && npm run test-common && npm run test-client",
    "test-server": "mocha --require server/helpers.test.js --recursive server/**/*.test.js",
    "test-common": "mocha --require server/helpers.test.js --recursive common/**/*.test.js",
    "test-client": "mocha --compilers js:babel-register --require client/helpers.test.js --require client/dom.test.js --recursive client/**/*.test.*",
    "build": "webpack --display-error-details --progress"
  },
  "private": true,
  "author": "Lewis Haggar",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.1",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.23.0",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "chai": "^3.5.0",
    "concurrently": "^3.3.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.2.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "rewire": "^2.5.2",
    "sass-loader": "^6.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
